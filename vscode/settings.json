{
    "breadcrumbs.enabled": true,
    "cSpell.userWords": [
        "account",
        "amortization",
        "amount",
        "an",
        "asset",
        "balance",
        "boolean",
        "borrower",
        "combined",
        "date",
        "employment",
        "enum",
        "executive",
        "famc",
        "ficos",
        "figure",
        "funded",
        "gnma",
        "in",
        "income",
        "injectable",
        "interest",
        "investor",
